{
  "name": "basster/silex-simple-swagger-provider",
  "description": "A silex service provider that integrates swagger-php 2.0 into silex",
  "keywords": [
    "silex",
    "swagger",
    "swagger-ui",
    "service",
    "provider"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ole Rößner",
      "email": "oroessner@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "silex/silex": "^1.3",
    "zircote/swagger-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.3"
  },
  "autoload": {
    "psr-4": {
      "Basster\\Silex\\Provider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Basster\\Silex\\Provider\\Test\\": "test/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors"
  },
  "minimum-stability": "stable"
}
